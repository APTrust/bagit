        "electronVersion": "1.7.10",
        "asar": true,
        "directories": {
            "app": "/Users/apd4n/go/src/github.com/APTrust/easy-store/electron",
            "output": "dist"
        },
        "mac": {
            "category": "public.app-category.productivity",
            "target": [
                {

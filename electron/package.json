{
    "name": "EasyStore",
    "version": "0.1.0",
    "main": "main.js",
    "dependencies": {
        "dateformat": "^3.0.2",
        "electron-context-menu": "^0.9.1",
        "electron-store": "^1.3.0",
        "handlebars": "^4.0.11",
        "minio": "^3.2.3",
        "require-dir": "^0.3.2"
    },
    "devDependencies": {
